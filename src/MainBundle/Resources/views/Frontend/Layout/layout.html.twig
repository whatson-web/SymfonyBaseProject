<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block metas %}{% endblock %}

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    {% stylesheets
    'front/plugins/bootstrap/css/bootstrap.min.css'
    "front/plugins/bootstrap-datepicker/css/bootstrap-datepicker.min.css"
    "front/plugins/select2/css/select2.min.css"
    "front/plugins/slick/css/slick.css"
    filter="cssrewrite" %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

    {% stylesheets
    filter="scssphp" %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

    {# Ajouter des .css ou du css dans une vue #}
    {% block stylesheets %}{% endblock %}

</head>

<body>

{% block body %}{% endblock %}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

{% javascripts
'front/plugins/bootstrap/js/bootstrap.min.js'
'front/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js'
'front/plugins/bootstrap-datepicker/locales/bootstrap-datepicker.fr.min.js'
'front/plugins/fresco/js/fresco/fresco.js'
'front/plugins/select2/js/select2.min.js'
'front/plugins/slick/js/slick.min.js'
'front/whatson/js/layout.js' %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{# Ajouter des .js dans une vue #}
{% block javascripts %}{% endblock %}

{# Ajouter du js dans une vue #}
{% block javascript %}{% endblock %}

</body>
</html>